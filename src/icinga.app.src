%%% -*- mode: erlang -*-
{application, icinga,
 [
  {description, "Erlang Icinga Client"},
  {vsn, "0.1.0"},
  {registered, []},
  {applications, [
                  kernel,
                  stdlib
                 ]},
  {mod, { icinga_app, []}},
  {env, [
         {server_hostname, "localhost"},
         {client_hostname, "localhost"},
         {send_ncsa_executable, "/usr/sbin/send_nsca"},
         {send_ncsa_config, "/etc/send_nsca.cfg"}
        ]
  }
 ]}.
